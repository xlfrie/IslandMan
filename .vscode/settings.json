{
	"[javascript]": {
		"editor.defaultFormatter": "esbenp.prettier-vscode"
	},
	"[typescript]": {
		"editor.defaultFormatter": "esbenp.prettier-vscode"
	},
	"git.ignoreLimitWarning": true,
	"editor.formatOnSave": true,
	"search.exclude": {
		"**/.git": true,
		"**/node_modules": true,
		"**/build": true,
		"**/bds": true
	},
	"files.exclude": {
		"**/.git": true,
		"**/.DS_Store": true,
		"**/node_modules": true,
		"**/build": true,
		"**/bds": true
	},
	"prettier.useTabs": true,
	"typescript.preferences.importModuleSpecifierEnding": "minimal"
}
